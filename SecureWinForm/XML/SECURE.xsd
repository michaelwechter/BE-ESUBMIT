      <xs:element name="RequireCityIndex" type="xs:boolean" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The specified County requires the Cities collection index.
          </xs:documentation>
        </xs:annotation>
