<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.secure-recording.com/schemas"
    elementFormDefault="qualified"
    xmlns="http://www.secure-recording.com/schemas"
    xmlns:secure="http://www.secure-recording.com/schemas"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    version="3.5.0.0"
>

  <xs:element name="Authentication" type="secure:AuthenticationDetail">
    <xs:annotation>
      <xs:documentation>
        The Authentication element contains information detailing a users authentication credentials. The Authentication element is used when a user logins to the SECURE System.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="Session" type="secure:SessionDetail">
    <xs:annotation>
      <xs:documentation>
        The Session element contains information detailing the users active session. The Session element is returned when a user logins successfully to the SECURE System.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="BatchWithDocuments" type="secure:BatchWithDocumentsDetail">
    <xs:annotation>
      <xs:documentation>
        The Batch with Documents element collection contains a complete batch including a collection of all Document Details.  This element should only be used to bulk upload and download a batch. 
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Batches" type="secure:BatchesDetail">
    <xs:annotation>
      <xs:documentation>
        The Batches element contains a collection of Batch elements.  The Batches element is used to read one or more Batch elements from the SECURE System.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="Batch" type="secure:BatchDetail">
    <xs:annotation>
      <xs:documentation>
        The Batch element contains a collection of recordable documents, PCORs, and other supporting documentation transmitted to the County Recorder.  The Document node in the Batch element only contains the Document meta-data, and it does not include the image data (payload).
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Document" type="secure:DocumentDetail">
    <xs:annotation>
      <xs:documentation>
        The Document element contains the document, PCORs or other supporting documentation transmitted to the County Recorder. The Document element contains the image data (payload).
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Titles" type="secure:TitlesDetail">
    <xs:annotation>
      <xs:documentation>
        The Titles element contains a collection of document recording titles assigned by each County.  The Titles index is optional for each County. Please see the Index Options to determine if the Titles index is enable for the target County.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="Cities" type="secure:CitiesDetail">
    <xs:annotation>
      <xs:documentation>
        The Cities element contains a collection of cities for each County.  It identifies the city where the transfer of interest in real property occurred.  The Cities index is optional for each County. Please see the Index Options to determine if the Cities Index enable for the target County.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="ProcessQueues" type="secure:ProcessQueuesDetail">
    <xs:annotation>
      <xs:documentation>
        The Process Queues element contains the list of available work queues that are defined by each County where Submitting Parties submit batches of documents for examination and recording.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="Counties" type="secure:CountiesInfo">
    <xs:annotation>
      <xs:documentation>
        The Counties element contains the list of available County elements in the SECURE v3 System.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="County" type="secure:CountyInfo">
    <xs:annotation>
      <xs:documentation>
        The County element contains information about the specific County including the system ID, name, and FIPS codes.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="RequestingParties" type="secure:RequestingPartiesInfo">
    <xs:annotation>
      <xs:documentation>
        The Requesting Parties element contains the list of available Requesting Parties for a specified Submitting Party.  The Requesting Parties index is optional for each County. Please see the Index Options to determine if the Requesting Parties Index enable for the target County.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="SubmittingParties" type="secure:SubmittingPartiesInfo">
    <xs:annotation>
      <xs:documentation>
        The Submitting Parties element contains the list of available Submitting Parties for a County.  The Submitting Party is only accessible by the target County if the Submitting Party has been granted access to one or more of the Counties Process Queues.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="IndexOptions" type="secure:IndexOptionDetail">
    <xs:annotation>
      <xs:documentation>
        The Index Options element contains the configurable batch and document indices that enabled by each County.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Result" type="secure:ResultDetail">
    <xs:annotation>
      <xs:documentation>
        The Result element contains the Boolean (true or false) return value.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="AuthenticationDetail">
    <xs:annotation>
      <xs:documentation>
        The user's authentication credentials used to login to the system.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence maxOccurs="1" minOccurs="1">
      <xs:element name="UserName">
        <xs:annotation>
          <xs:documentation>
            The unique sequence of characters used to identify a user and allow access to the system.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Password">
        <xs:annotation>
          <xs:documentation>
            The unique sequence of characters used by authorized persons to prove their right to access the system.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="256"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="NewPassword" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Used to change the user's password. Field is ignored on login.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="256"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="TwoFactorPassword" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The user's one time token key.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="256"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SessionDetail">
    <xs:annotation>
      <xs:documentation>
        The Session information detailing the users active session.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence maxOccurs="1" minOccurs="1">
      <xs:element name="SessionTokenID">
        <xs:annotation>
          <xs:documentation>
            The Session Token ID used to identify the users active session.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="128"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PasswordExpired" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            The user's password has expired and must be changed before additional using the system.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FirstName" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The first name of the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LastName" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The last name of the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UserType" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The type of the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="BatchWithDocumentsDetail">
    <xs:annotation>
      <xs:documentation>
        A collection that contains the complete batch including all associated documents.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Batch" type="secure:BatchDetail" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A collection of recordable documents, PCORs, and other supporting documentation transmitted to the County Recorder as a Batch.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Documents" type="secure:DocumentsDetail" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A collection that contains contains bulk upload documents.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="BatchesDetail">
    <xs:annotation>
      <xs:documentation>
        A collection that contains submitted batches.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Batch" type="secure:BatchDetail" maxOccurs="100">
        <xs:annotation>
          <xs:documentation>
            A collection that contains submitted batches.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="BatchDetail">
    <xs:annotation>
      <xs:documentation>
        A collection of recordable documents, PCORs, and other supporting documentation transmitted to the County Recorder as a Batch.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence maxOccurs="1" minOccurs="1">
      <xs:element name="Name">
        <xs:annotation>
          <xs:documentation>
            The name given to batch by the Submitter.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="County" type="secure:CountyInfo">
        <xs:annotation>
          <xs:documentation>
            The meta-data for a County where the Batch was submitted.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcessQueue" type="secure:ProcessQueueDetail">
        <xs:annotation>
          <xs:documentation>
            The meta-data for a County process queue where the Batch was submitted.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The reference number for the point-of-sale transaction that is associated with the batch.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="StatusCode" type="secure:BatchMediaStatusCode">
        <xs:annotation>
          <xs:documentation>
            The codes that uniquely identify status of the Batch as the Batch transitions through the states of transmittal and recording.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CheckedOutBy" type="secure:UserInfo" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The user meta-data for the system user that has the Batch checked out.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CheckedOutTimeStamp" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The date/time the Batch was checked out by the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>    
      <xs:element name="SubmittingParty" type="secure:SubmittingPartyInfo" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Contains the meta-data for the Submitting Party that is submitting documents to the County.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RequestingParty" type="secure:RequestingPartyInfo" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Contains the meta-data for a Requesting Party identified for the Batch.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IsConcurrent" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            The flag indicating that the Batch has documents that need to be recorded concurrently with another Batch.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CreateTimeStamp" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The date/time the Batch was created.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EditTimeStamp" type="xs:dateTime" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The date/time the Batch was last edited.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Memos" type="BatchMemosDetail" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            A collection that contains the memos for a Batch.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Documents" type="secure:DocumentsInfo">
        <xs:annotation>
          <xs:documentation>
            A collection that contains meta-data for the documents in a Batch.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="_ID" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>
          The unique identifier assigned by the SECURE System for the Batch.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="_SubmissionID" type="secure:Guid" use="required">
      <xs:annotation>
        <xs:documentation>
          The submission ID of the batch provided by the submitter.  Used to enforce duplicate submission prevention scheme.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="CountiesInfo">
    <xs:annotation>
      <xs:documentation>
        A collection of available Counties for a specified Submitting Party.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="County" type="secure:CountyInfo" maxOccurs="100">
        <xs:annotation>
          <xs:documentation>
            A collection of available Counties for a specified Submitting Party.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="CountyInfo">
    <xs:annotation>
      <xs:documentation>
        Contains the meta-data for a County.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The name of the County.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="75"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FipsStateCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The Federal Information Processing Standard (FIPS) code of the State.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:length value="2"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FipsCountyCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The Federal Information Processing Standard (FIPS) code of the County.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:length value="3"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Logo" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The logo of the County encoded in Base64.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="_ID" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>
          The unique identifier assigned by the SECURE System for the County.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="ProcessQueuesDetail">
    <xs:annotation>
      <xs:documentation>
        A collection of available Process Queues for a specified Submitting Party.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ProcessQueue" type="secure:ProcessQueueDetail" maxOccurs="100">
        <xs:annotation>
          <xs:documentation>
            A collection of available Process Queues for a specified Submitting Party.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ProcessQueueDetail">
    <xs:annotation>
      <xs:documentation>
        Contains the meta-data for a County process queue.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>      
      <xs:element name="Name" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The name of the Process Queue provided by the County.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Description" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The description of the Process Queue provided by the County.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="OpenTime" type="xs:time" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The time the Process Queue opens (Military Time) for batch submission to the County.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CloseTime" type="xs:time" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The time the Process Queue closes (Military Time).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="_ID" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>
          The unique identifier assigned by the SECURE System for the Process Queue.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="_CountyID" type="xs:integer" use="optional">
      <xs:annotation>
        <xs:documentation>
          The unique identifier assigned by the SECURE System for the County.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  
  <xs:complexType name="UserInfo">
    <xs:annotation>
      <xs:documentation>
        Contains the meta-data for a system user.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="UserName" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The system user name of the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FirstName" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The first name of the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LastName" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The last name of the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SubmittingPartiesInfo">
    <xs:annotation>
      <xs:documentation>
        A collection of available Submitting Parties for a specified County.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SubmittingParty" type="secure:SubmittingPartyInfo" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            A collection of available Submitting Parties for a specified County.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SubmittingPartyInfo">
    <xs:annotation>
      <xs:documentation>
        Contains the meta-data for the Submitting Party that is submitting documents to the County.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The name of the Submitting Party.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PartyType" type="secure:PartyTypes" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The type of the Submitting Party (Agent, Authorized_Submitter or Government_Agency).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="_ID" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>
          The unique identifier assigned by the SECURE System for the Submitting Party.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="RequestingPartiesInfo">
    <xs:annotation>
      <xs:documentation>
        A collection of available Requesting Parties for a specified Submitting Party.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RequestingParty" type="secure:RequestingPartyInfo" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            A collection of available Requesting Parties for a specified Submitting Party.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>  

  <xs:complexType name="RequestingPartyInfo">
    <xs:annotation>
      <xs:documentation>
        Contains the meta-data for a Requesting Party.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The name of the Requesting Party.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IsChargingParty" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            This is checked if the requesting party will be charged for the documents.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="_ID" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>
          The unique identifier assigned by the SECURE System for the Requesting Party.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="DocumentsInfo">
    <xs:annotation>
      <xs:documentation>
        A collection that contains meta-data for the documents in a batch.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Document" type="secure:DocumentInfo" maxOccurs="100">
        <xs:annotation>
          <xs:documentation>
            A collection that contains meta-data for the documents in a batch.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="DocumentInfo">
    <xs:annotation>
      <xs:documentation>
        A document including indicies, recordable document, PCORs, and other supporting documentation transmitted to the County.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Number" type="xs:long" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The document number, instrument number, or book and page number (concatenated) for the document assigned by the County.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecordedTimeStamp" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The date/time the document was recorded by the County.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecordableFileContentInfo" type="secure:FileContentInfo">
        <xs:annotation>
          <xs:documentation>
            Contains document meta-data for the Recordable Document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PCORFileContentInfo" type="secure:FileContentInfo" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Contains document meta-data for the Preliminary Change of Ownership Record (PCOR) document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
