  <xs:complexType name="NamesDetail">
    <xs:annotation>
      <xs:documentation>
        A collection that contains the Names associated with a Recordable Document.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="secure:NameDetail" minOccurs="0" maxOccurs="100">
        <xs:annotation>
          <xs:documentation>
            A collection that contains the Names associated with a Recordable Document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="NameDetail">
    <xs:annotation>
      <xs:documentation>
        Contains a Name associated with a Recordable Document.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Type" type="secure:NameTypes">
        <xs:annotation>
          <xs:documentation>
            The type of the name (Grantor, Grantee, or Other).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The value of the Name associated with the Document. This is the full unparsed name of the (Grantor, Grantee, or Other).
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="150"/>            
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Prefix" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The prefix value of the (Grantor, Grantee, or Other) name.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FirstName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The first name value of the (Grantor, Grantee, or Other) name.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="MiddleName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The middle name value of the (Grantor, Grantee, or Other) name.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LastName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The last name value of the (Grantor, Grantee, or Other) name.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Suffix" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The suffix value of the (Grantor, Grantee, or Other) name.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ActionCode" type="secure:ActionCode">
        <xs:annotation>
          <xs:documentation>
            The action code for the Name Record (None, New, Edit, Delete).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CreateTimeStamp" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The date/time the Name record was created.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EditTimeStamp" type="xs:dateTime" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The date/time the Name record was edited.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="_ID" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>
          The unique identifier assigned by the SECURE System for the individual Document Name Record.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:simpleType name="EncodingTypes">
    <xs:annotation>
      <xs:documentation>
        The types of file encoding supported by the SECURE System.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Base64">
        <xs:annotation>
          <xs:documentation>
            The embedded file encoded using Base64.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="NameTypes">
    <xs:annotation>
      <xs:documentation>
        The types of document index names supported by the SECURE System.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Grantor">
        <xs:annotation>
          <xs:documentation>
            The type of the document index name is a grantor.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Grantee">
        <xs:annotation>
          <xs:documentation>
            The type of the document index name is a grantee.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Other">
        <xs:annotation>
          <xs:documentation>
            The type of the document index name is not a grantor or a grantee.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="MemoTypes">
    <xs:annotation>
      <xs:documentation>
        The types of memos supported by the SECURE System.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Note">
        <xs:annotation>
          <xs:documentation>
            The type of the memo is a note.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Reject_Reason">
        <xs:annotation>
          <xs:documentation>
            The type of the memo is a reject reason from the County.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>     
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name ="EmbededFileTypes">
    <xs:annotation>
      <xs:documentation>
        The file types supported by the SECURE System.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Single_Page_Tiff">
        <xs:annotation>
          <xs:documentation>
            The embedded file type of the document payload is Single Page TIFFs.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Multi_Page_Tiff">
        <xs:annotation>
          <xs:documentation>
            The embedded file type of the document payload is a Multi-page TIFF.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PRIA_XML">
        <xs:annotation>
          <xs:documentation>
            The embedded file type of the document payload is a PRIA XML document. [For future use, not currently supported by any of the Counties.]
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DocumentMediaStatusCode">
    <xs:annotation>
      <xs:documentation>
        The codes that uniquely identify status of the Document as the Document transitions through the states of transmittal and recording.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="New">
        <xs:annotation>
          <xs:documentation>
            The document payload is new and has not been uploaded.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Uploaded">
        <xs:annotation>
          <xs:documentation>
            The document payload has been uploaded by the Submitter to SECURE for examination by the County.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Correction">
        <xs:annotation>
          <xs:documentation>
            The document payload has been uploaded by the County to SECURE for correction by the Submitter.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Corrected">
        <xs:annotation>
          <xs:documentation>
            The corrected document payload has been uploaded by the Submitter to SECURE for re-examination by the County.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Rejected">
        <xs:annotation>
          <xs:documentation>
            The document payload has been rejected by the County.  The document cannot be corrected.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Recorded">
        <xs:annotation>
          <xs:documentation>
            The document payload has been recorded by the County.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Purged">
        <xs:annotation>
          <xs:documentation>
            The document payload has been purged from SECURE.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="BatchMediaStatusCode">
    <xs:annotation>
      <xs:documentation>
        The codes that uniquely identify status of the Batch as the Batch transitions through the states of transmittal and recording.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">      
      <xs:enumeration value="New">
        <xs:annotation>
          <xs:documentation>
            The batch is new and has not been submitted.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Submitter_Uploading">
        <xs:annotation>
          <xs:documentation>
            The Submitter is uploading the batch to SECURE for examination by the County.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Uploaded_By_Submitter">
        <xs:annotation>
          <xs:documentation>
            The Submitter has uploaded the batch to SECURE for examination by the County.  The batch is waiting to be downloaded by the County for examination.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Downloaded_By_County">
        <xs:annotation>
          <xs:documentation>
            The batch has been downloaded by the County for examination.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="County_Uploading_Correction">
        <xs:annotation>
          <xs:documentation>
            The County is uploading the batch to SECURE for correction by the Submitter.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Correction_Uploaded_By_County">
        <xs:annotation>
          <xs:documentation>
            The County has uploaded the batch to SECURE for correction by the Submitter.  The batch is waiting to be downloaded by the Submitter for correction.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Correction_Downloaded_By_Submitter">
        <xs:annotation>
          <xs:documentation>
            The batch has been downloaded by the Submitter for correction.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Submitter_Uploading_Corrected">
        <xs:annotation>
          <xs:documentation>
            The Submitter is uploading the corrected batch to SECURE for re-examination by the County.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Corrected_Uploaded_By_Submitter">
        <xs:annotation>
          <xs:documentation>
            The Submitter has uploaded the corrected batch to SECURE for re-examination by the County.  The batch is waiting to be downloaded by the County for re-examination.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Corrected_Downloaded_By_County">
        <xs:annotation>
          <xs:documentation>
            The corrected batch has been downloaded by the County for re-examination.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="County_Uploading_Completed">
        <xs:annotation>
          <xs:documentation>
            The County is uploading the completed batch to SECURE for the Submitter.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Completed_Uploaded_By_County">
        <xs:annotation>
          <xs:documentation>
            The County has uploaded the completed batch to SECURE for the Submitter.  The batch is waiting to be downloaded by the Submitter.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Completed_Downloaded_By_Submitter">
        <xs:annotation>
          <xs:documentation>
            The completed batch has been downloaded by the Submitter.  The processing of this batch is complete.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Purged">
        <xs:annotation>
          <xs:documentation>
            The document payloads have been purged from SECURE for the batch.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ActionCode">
    <xs:annotation>
      <xs:documentation>
        These codes inform the SECURE System that one of the following actions is intended on the data.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="None">
        <xs:annotation>
          <xs:documentation>
            The data in the element has not been changed.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="New">
        <xs:annotation>
          <xs:documentation>
            The data in the element is new.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Edit">
        <xs:annotation>
          <xs:documentation>
            The data in the element has been modified.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Delete">
        <xs:annotation>
          <xs:documentation>
            The data in the element is flagged for deletion.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PartyTypes">
    <xs:annotation>
      <xs:documentation>
        The type of the Submitting Party (Agent, Authorized_Submitter or Government Agency).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Agent">
        <xs:annotation>
          <xs:documentation>
            The type of party is an Agent.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Authorized_Submitter">
        <xs:annotation>
          <xs:documentation>
            The type of party is an Authorized Submitter.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Government_Agency">
        <xs:annotation>
          <xs:documentation>
            The type of party is a Government Agency.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="Money">
    <xs:annotation>
      <xs:documentation>
        The supported money format in the SECURE System.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:totalDigits value="17"/>
      <xs:fractionDigits value="2"/>
      <xs:maxInclusive value="922337203685477.58"/>
      <xs:minInclusive value="-922337203685477.58"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Guid">
    <xs:annotation>
      <xs:documentation>
        The supported GUID format in the SECURE System.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})|(\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\})"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Memo">
    <xs:annotation>
      <xs:documentation>
        The supported memo string format in the SECURE System.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="8000"/>
    </xs:restriction>
  </xs:simpleType>

</xs:schema>